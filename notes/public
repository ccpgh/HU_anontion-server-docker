# base64 eencoded SPKI-SHA256-BASE64 
openssl s_client -showcerts anontion.com:443 | openssl x509 -outform PEM | openssl x509 -inform pem -noout -outform pem -pubkey | openssl pkey -pubin -inform pem -outform der | openssl dgst -sha256 -binary | openssl enc -base64

